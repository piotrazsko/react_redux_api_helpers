{
	"name": "react_redux_api",
	"description": "Collection of actions, reducers and helpers for work with API",
	"version": "0.0.9",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"private": false,
	"license": "MIT",
	"author": "Siarhei Petrashka",
	"scripts": {
		"start": "node index.ssl.js",
		"clean": "rimraf dist",
		"build": "tsc && vite build",
		"build:prod": "yarn run version:patch && yarn build",
		"type-check": "tsc --noEmit",
		"test": "vitest",
		"test:run": "vitest run",
		"version:patch": "yarn build && git add . && yarn patch",
		"version:feature": "yarn build && git add . && yarn feature",
		"version:release": "yarn build && git add . && yarn release",
		"patch": "yarn version --patch",
		"feature": "yarn version --minor",
		"release": "yarn version --major"
	},
	"dependencies": {
		"axios": "^1.5.0",
		"memoize-state": "^2.0.15",
		"redux-actions": "^2.6.5",
		"redux-saga": "^1.2.3"
	},
	"optionalDependencies": {
		"connected-react-router": "^5.0.1",
		"jwt-decode": "^2.2.0",
		"lodash": "^4.17.21",
		"react-cookie": "^2.1.1",
		"redux-pack": "^0.1.5",
		"redux-saga-test-plan": "^3.7.0"
	},
	"peerDependencies": {
		"redux-saga": ">=^0.16.0"
	},
	"devDependencies": {
		"@redux-saga/types": "^1.2.1",
		"@types/jest": "^29.5.5",
		"@types/node": "^20.6.3",
		"@types/redux-actions": "^2.6.2",
		"express": "^4.18.2",
		"history": "^5.3.0",
		"jsdom": "^22.1.0",
		"less": "^4.2.0",
		"pug": "^3.0.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.1.2",
		"react-router-dom": "^6.15.0",
		"redux": "^4.2.1",
		"redux-logger": "^3.0.6",
		"redux-persist": "^6.0.0",
		"rimraf": "^5.0.1",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-plugin-dts": "^3.5.3",
		"vitest": "^0.34.4"
	}
}
